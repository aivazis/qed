# impish-gcc image configuration
qed.ci.impish-gcc.name := impish-gcc
qed.ci.impish-gcc.root := etc/docker/ci/
qed.ci.impish-gcc.launch.mounts := mm pyre qed data
# qed listens on port 8005, by default; map it to port 8006 on the host
qed.ci.impish-gcc.launch.options := -p 8006:8005


