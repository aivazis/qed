# impish-gcc image configuration
qed.dev.impish-gcc.name := impish-gcc
qed.dev.impish-gcc.root := etc/docker/dev/
qed.dev.impish-gcc.launch.mounts := mm pyre qed data
# qed listens on port 8005, by default; map it to port 8006 on the host
qed.dev.impish-gcc.launch.options := -p 8006:8005


