# lunar-gcc image configuration
qed.ci.lunar-gcc.name := lunar-gcc
qed.ci.lunar-gcc.root := etc/docker/ci/
qed.ci.lunar-gcc.launch.mounts := mm pyre qed data
# qed listens on port 8005, by default; map it to port 8006 on the host
qed.ci.lunar-gcc.launch.options := -p 8006:8005


