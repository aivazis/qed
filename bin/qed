# turn some debug channels on
import journal
journal.debug("qed.ux.graphql").activate()


# get the package
import qed

# build an instance of the plexus
app = qed.shells.qed(name='qed.app')
# and run it
status = app.run()
# pass the status on to the shell
raise SystemExit(status)


# end of file
